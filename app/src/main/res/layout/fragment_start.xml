<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.trent.sleepapp.StartFragment">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:text="Take PVT, PAM and SSS Tests"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bTests"
            android:layout_marginTop="60dp"
            android:layout_centerHorizontal="true"
            />

        <Button
            android:text="Take LEEDS Test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bLEEDS"
            android:layout_marginTop="120dp"
            android:layout_centerHorizontal="true"
            />

        <Button
            android:text="Take PANAS Test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bPANAS"
            android:layout_marginTop="180dp"
            android:layout_centerHorizontal="true"
            />
    </RelativeLayout>


</FrameLayout>
